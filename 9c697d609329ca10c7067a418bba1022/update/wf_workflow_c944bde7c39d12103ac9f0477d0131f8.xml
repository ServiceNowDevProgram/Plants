        <value>var type = current.variables.request_type.getValue();&#13;
var plt = current.variables.plant.getValue();&#13;
if(type == 'create_new_plant')&#13;
{&#13;
	var getPlant = new GlideRecord('x_snc_plant_mgmt_plants');&#13;
	getPlant.initialize();&#13;
&#13;
	getPlant.name = current.variables.name.getValue();&#13;
	getPlant.planting_method = current.variables.planting_method.getValue();&#13;
	getPlant.description = current.variables.description.getValue();&#13;
	getPlant.pottery_type = current.variables.pottery_type.getValue();&#13;
	getPlant.date_planted = current.variables.date_planted.getValue();&#13;
	getPlant.amount_of_water_required_mm = current.variables.amount_of_water_required.getValue();&#13;
&#13;
	getPlant.insert();&#13;
}&#13;
if(type == 'update_existing_plant')&#13;
{&#13;
	var updatePlant = new GlideRecord('x_snc_plant_mgmt_plants');&#13;
	if(updatePlant.get(plt))&#13;
	{&#13;
		updatePlant.name = current.variables.name_2.getValue();&#13;
		updatePlant.planting_method = current.variables.planting_method_2.getValue();&#13;
		updatePlant.description = current.variables.description_2.getValue();&#13;
		updatePlant.pottery_type = current.variables.pottery_type_2.getValue();&#13;
		updatePlant.date_planted = current.variables.date_planted_2.getValue();&#13;
		updatePlant.amount_of_water_required_mm = current.variables.amount_of_water_required_2.getValue();&#13;
&#13;
		updatePlant.update();&#13;
	}&#13;
}</value>
